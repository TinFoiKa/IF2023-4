/* pages/BoothLists/BoothLists.wxss */
.uni-list {
    background-color: #fff;
    position: relative;
    width: 100%;
    display: flex;
    flex-direction: column;
  }
  .uni-list-cell {
    position: relative;
    display: flex;
    flex-direction: row;
    justify-content: space-between;
    align-items: center;
  }
  .uni-list-cell {
	padding-top: 30rpx;
  }
  
  .uni-media-list {
    padding: 11px 1px;
    margin-left: 5%;
    border-bottom: 1rpx solid #ddd;
    box-sizing: border-box;
    display: flex;
    width: 100%;
    flex-direction: row;
  }
  .uni-media-list-logo{
    width: 300rpx;
	height: 450rpx;
  }
  .uni-media-list-body{
    height: auto;
    justify-content: space-around;
  }
  .uni-media-list-body {
    min-height: 42px;
    display: flex;
    margin-left: 30rpx;
    flex: 1;
    flex-direction: column;
    justify-content: space-between;
    align-items: flex-start;
    overflow: hidden;
  }
  .uni-media-list-text-top{
    min-height: 37px;
  
    font-size: 14px;
    overflow: hidden;
  }
  .uni-media-list-text-top {
    width: 100%;
    line-height: 18px;
    font-size: 15px;
  }
  .uni-media-list-text-bottom{
    display: flex;
    flex-direction: row;
    justify-content: space-between;
  }
  
  
  .uni-media-list-text-bottom {
    width: 100%;
    line-height: 15px;
    font-size: 30rpx;
	color: #8f8f94;
	
  }

  .uni-media-go{
	width: 30rpx;
	height: 30rpx;
	padding-right: 30rpx;
  }