<!--pages/BoothLists/BoothLists.wxml-->
<view class="content">
    <my-top toptitle='Booth'></my-top>
	<view class="container">
		<view class="credits" style="height: 500rpx;">
			<view class="credits-title" style="text-align: left;margin-left: 60rpx;font-family: jiansong-bold;font-weight: 500;">CULTURE FOODS</view>
			<view style="display: flex;justify-content: space-between;height: 80%;">
				<view class="credits-content" style="margin-right: 5rpx;height: 100%;">
					<image class="img-style" mode="scaleToFill" src="{{activityEvent.foodUrl}}" wx:if="{{activityEvent.foodUrl != undefined && activityEvent.foodUrl != null && activityEvent.foodUrl != ''}}"></image>
				</view>
				<view class="credits-content" style="margin-left: 5rpx;height: 100%;">
					<scroll-view scroll-y="true" class="notice-content" style="height: 340rpx;margin-bottom: 0rpx;">
						<text style="font-family: jiansong-regular;">{{activityEvent.foodDesc}}</text>
					</scroll-view>
					<view bindtap="foods" style="width: 100%;height: 60rpx;background-color: #E77B0B;border-radius: 15rpx;">
						<view style="width: 100%;height: 100%;text-align: center;color: #FFFFFF;font-size: 40rpx;font-weight: bold;">Explore</view>
					</view>
				</view>
			</view>
		</view>

		<view class="credits" style="height: 500rpx;">
			<view class="credits-title" style="text-align: left;margin-left: 60rpx;font-family: jiansong-bold;font-weight: 500;">GAMES</view>
			<view style="display: flex;justify-content: space-between;height: 80%;">
				<view class="credits-content" style="margin-right: 5rpx;height: 100%;">
					<image class="img-style" mode="scaleToFill" src="{{activityEvent.gameUrl}}" wx:if="{{activityEvent.gameUrl != undefined && activityEvent.gameUrl != null && activityEvent.gameUrl != ''}}"></image>
				</view>
				<view class="credits-content" style="margin-left: 5rpx;height: 100%;">
					<scroll-view scroll-y="true" class="notice-content" style="height: 340rpx;margin-bottom: 0rpx;">
						<text style="font-family: jiansong-regular;">{{activityEvent.gameDesc}}</text>
					</scroll-view>
					<view bindtap="games" style="width: 100%;height: 60rpx;background-color: #E77B0B;border-radius: 15rpx;">
						<view style="width: 100%;height: 100%;text-align: center;color: #FFFFFF;font-size: 40rpx;font-weight: bold;">Explore</view>
					</view>
				</view>
			</view>
		</view>

		<view class="credits" style="height: 500rpx;">
			<view class="credits-title" style="text-align: left;margin-left: 60rpx;font-family: jiansong-bold;font-weight: 500;">MAIN STAGE PERFORMANCES</view>
			<view style="display: flex;justify-content: space-between;height: 80%;">
				<view class="credits-content" style="margin-right: 5rpx;height: 100%;">
					<image class="img-style" mode="scaleToFill" src="{{activityEvent.performancesMainUrl}}" wx:if="{{activityEvent.performancesMainUrl != undefined && activityEvent.performancesMainUrl != null && activityEvent.performancesMainUrl != ''}}"></image>
				</view>
				<view class="credits-content" style="margin-left: 5rpx;height: 100%;">
					<scroll-view scroll-y="true" class="notice-content" style="height: 340rpx;margin-bottom: 0rpx;">
						<text style="font-family: jiansong-regular;">{{activityEvent.performancesMainDesc}}</text>
					</scroll-view>
					<view bindtap="performances" data-type="3" style="width: 100%;height: 60rpx;background-color: #E77B0B;border-radius: 15rpx;">
						<view style="width: 100%;height: 100%;text-align: center;color: #FFFFFF;font-size: 40rpx;font-weight: bold;">Explore</view>
					</view>
				</view>
			</view>
		</view>

		<view class="credits" style="height: 500rpx;">
			<view class="credits-title" style="text-align: left;margin-left: 60rpx;font-family: jiansong-bold;font-weight: 500;">HILLSIDE STAGE PERFORMANCES</view>
			<view style="display: flex;justify-content: space-between;height: 400rpx;">
				<view class="credits-content" style="margin-right: 5rpx;height: 100%;">
					<image class="img-style" mode="scaleToFill" src="{{activityEvent.performancesHillsideUrl}}" wx:if="{{activityEvent.performancesHillsideUrl != undefined && activityEvent.performancesHillsideUrl != null && activityEvent.performancesHillsideUrl != ''}}"></image>
				</view>
				<view class="credits-content" style="margin-left: 5rpx;">
					<scroll-view scroll-y="true" class="notice-content" style="height: 340rpx;margin-bottom: 0rpx;">
						<text style="font-family: jiansong-regular;">{{activityEvent.performancesHillsideDesc}}</text>
					</scroll-view>
					<view bindtap="performances" data-type="4" style="width: 100%;height: 60rpx;background-color: #E77B0B;border-radius: 15rpx;">
						<view style="width: 100%;height: 100%;text-align: center;color: #FFFFFF;font-size: 40rpx;font-weight: bold;">Explore</view>
					</view>
				</view>
			</view>
		</view>

		<view class="credits" style="height: 500rpx;">
			<view class="credits-title" style="text-align: left;margin-left: 60rpx;font-family: jiansong-bold;font-weight: 500;">VENDORS</view>
			<view style="display: flex;justify-content: space-between;height: 80%;">
				<view class="credits-content" style="margin-right: 5rpx;height: 100%;">
					<image class="img-style" mode="scaleToFill" src="{{activityEvent.vendorUrl}}" wx:if="{{activityEvent.vendorUrl != undefined && activityEvent.vendorUrl != null && activityEvent.vendorUrl != ''}}"></image>
				</view>
				<view class="credits-content" style="margin-left: 5rpx;height: 100%;">
					<scroll-view scroll-y="true" class="notice-content" style="height: 340rpx;margin-bottom: 0rpx;">
						<text style="font-family: jiansong-regular;">{{activityEvent.vendorDesc}}</text>
					</scroll-view>
					<view bindtap="vendors" data-type="2" style="width: 100%;height: 60rpx;background-color: #E77B0B;border-radius: 15rpx;">
						<view style="width: 100%;height: 100%;text-align: center;color: #FFFFFF;font-size: 40rpx;font-weight: bold;">Explore</view>
					</view>
				</view>
			</view>
		</view>

		<view class="credits" style="height: 500rpx;">
			<view class="credits-title" style="text-align: left;margin-left: 60rpx;font-family: jiansong-bold;font-weight: 500;">STUDENT MARKET PLACE</view>
			<view style="display: flex;justify-content: space-between;height: 80%;">
				<view class="credits-content" style="margin-right: 5rpx;height: 100%;">
					<image class="img-style" mode="scaleToFill" src="{{activityEvent.studentMarketPlaceUrl}}" wx:if="{{activityEvent.studentMarketPlaceUrl != undefined && activityEvent.studentMarketPlaceUrl != null && activityEvent.studentMarketPlaceUrl != ''}}"></image>
				</view>
				<view class="credits-content" style="margin-left: 5rpx;height: 100%;">
					<scroll-view scroll-y="true" class="notice-content" style="height: 340rpx;margin-bottom: 0rpx;">
						<text style="font-family: jiansong-regular;">{{activityEvent.studentMarketPlaceDesc}}</text>
					</scroll-view>
					<view bindtap="vendors" data-type="3" style="width: 100%;height: 60rpx;background-color: #E77B0B;border-radius: 15rpx;">
						<view style="width: 100%;height: 100%;text-align: center;color: #FFFFFF;font-size: 40rpx;font-weight: bold;">Explore</view>
					</view>
				</view>
			</view>
		</view>

		<view class="credits" style="height: 500rpx;">
			<view class="credits-title" style="text-align: left;margin-left: 60rpx;font-family: jiansong-bold;font-weight: 500;">SPONSORS</view>
			<view style="display: flex;justify-content: space-between;height: 80%;">
				<view class="credits-content" style="margin-right: 5rpx;height: 100%;">
					<image class="img-style" mode="scaleToFill" src="{{activityEvent.sponsorUrl}}" wx:if="{{activityEvent.sponsorUrl != undefined && activityEvent.sponsorUrl != null && activityEvent.sponsorUrl != ''}}"></image>
				</view>
				<view class="credits-content" style="margin-left: 5rpx;height: 100%;">
					<scroll-view scroll-y="true" class="notice-content" style="height: 340rpx;margin-bottom: 0rpx;">
						<text style="font-family: jiansong-regular;">{{activityEvent.sponsorDesc}}</text>
					</scroll-view>
					<view bindtap="sponsors" style="width: 100%;height: 60rpx;background-color: #E77B0B;border-radius: 15rpx;">
						<view style="width: 100%;height: 100%;text-align: center;color: #FFFFFF;font-size: 40rpx;font-weight: bold;">Explore</view>
					</view>
				</view>
			</view>
		</view>

		<view class="credits">
			<view style="height: 95%;">
				<view class="credits-content" style="margin-right: 5rpx;height: 100%;" wx:for="{{logoList}}" wx:key="this">
					<image class="img-style" mode="widthFix" src="{{item}}" ></image>
				</view>
			</view>
		</view>
	</view>
</view>
